{
  "name": "orangejelly-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "diagnose-sanity": "ts-node scripts/diagnose-sanity-crash.ts",
    "fix-sanity-types": "ts-node scripts/fix-sanity-field-types.ts",
    "migrate:claims": "ts-node scripts/migrate-claims-to-sanity.ts",
    "migrate:services": "ts-node scripts/migrate-services-content.ts",
    "migrate:landing-pages": "ts-node scripts/migrate-landing-pages.ts",
    "migrate:navigation": "ts-node scripts/migrate-navigation.ts",
    "migrate:related-links": "ts-node scripts/migrate-related-links.ts",
    "fix:sanity-keys": "tsx scripts/fix-missing-keys.ts",
    "migrate:all-content": "ts-node scripts/migrate-all-hardcoded-content.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
