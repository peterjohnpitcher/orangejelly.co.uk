{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["lhr1"],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "orangejelly.co.uk"
        }
      ],
      "destination": "https://www.orangejelly.co.uk/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}